---
import NavbarContainer from "../react/NavbarContainer";
const pages = [
  {
    name: "Home",
    link: "/",
  },
  {
    name: "Experience",
    link: "/experience",
  },
  {
    name: "Works",
    link: "/works",
  },
];
---

<div class="flex flex-col items-center w-full h-32 my-10">
  <div
    class="flex bg-primary shadow-primary-shiny text-primary shadow-primary text-white py-4 px-10 justify-between w-[22.5%] border-primary border rounded-full">
    {pages.map((page) => <NavbarContainer class="cursor-pointer">{page.name}</NavbarContainer>)}
  </div>
</div>
